<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style type="text/css">
body {
	padding: 1.5em;
	background: #F5F5F5;
}

table {
	border: 1px solid #EDEDED;
	font-size: 0.9em;
	box-shadow: 0 2px 5px rgba(0, 0, 0, .25);
	width: 100%;
	border-radius: 5px;
	overflow: hidden;
	border-collapse: collapse;
}

th {
	text-align: left;
}
td, th{
padding: 1em .5em;
vertical-align: middle;
}
td{
border-bottom: 1px solid rgba(0,0,0,1);
background: #fff;
}
thead {
	font-weight: bold;
	color: white;
	background: #73685d;
}
</style>
</head>
<body>
	<table><thead><tr><th>123</th></tr></thead>
	<tbody><tr><td>asd</td></tr></tbody>
	</table>
</body>
</html>