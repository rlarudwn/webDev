CREATE TABLE diary(
   no NUMBER, 
   id VARCHAR2(20),
   subject VARCHAR2(1000) CONSTRAINT diary_sub_nn NOT NULL,
   msg CLOB CONSTRAINT diary_msg_nn NOT NULL,
   year NUMBER(4),
   month NUMBER(2),
   day NUMBER(2),
   regdate DATE DEFAULT SYSDATE,
   CONSTRAINT diary_no_pk PRIMARY KEY(no),
   CONSTRAINT diary_id_fk FOREIGN KEY(id)
   REFERENCES member(id)
);
CREATE SEQUENCE diary_no_seq
      START WITH 1
      INCREMENT BY 1
      NOCYCLE
      NOCACHE;